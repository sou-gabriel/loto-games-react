{
  "extends": [
    "react-app",
    "react-app/jest",
    "standard",
    "standard-jsx",
    "standard-react"
  ],
  "plugins": [
    "only-warn"
  ],
  "rules": {
    "camelcase": "off",
    "react/react-in-jsx-scope": "off",
    "no-unused-vars": "off",
    "@typescript-eslint/no-unused-vars": ["error", {
      "ignoreRestSiblings": true
    }]
  }
}
